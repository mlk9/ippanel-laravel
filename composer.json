{
    "name": "mlk9/ippanel-laravel",
    "description": "Send SMS notification based on channel Laravel (Ippanel SMS service)",
    "license": "MIT",
    "authors": [
        {
            "name": "Mohammad Maleki",
            "email": "malekii24@outlook.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Mlk9\\Sms\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mlk9\\SmsPackage\\Tests\\": "tests"
        }
    },
    "require": {
        "php": ">=7.4",
        "illuminate/support": ">=6"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Mlk9\\Sms\\SmsServiceProvider"
            ],
            "aliases": {
                "Setting": "Mlk9\\Sms\\Facades\\Sms"
            }
        }
    },
    "require-dev": {
        "guzzlehttp/guzzle": ">=7",
        "phpunit/phpunit": ">=9",
        "orchestra/testbench": ">=4"
    }
}
